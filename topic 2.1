package program2;
//To check the impact on object when the variable is nonstatic
public class Test1
{
	int a2= 10;
	public static void main(String[] args)
	{
		Test1 obj1 = new Test1();
		Test1 obj2 = new Test1();
		
		System.out.println(obj1.a2);
		System.out.println(obj2.a2);
		
		obj2.a2= 30;
		
		System.out.println(obj1.a2);
		System.out.println(obj2.a2);
	}

}
